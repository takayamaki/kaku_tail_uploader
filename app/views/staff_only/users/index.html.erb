<div class="container">
  <table class='table is-fullwidth is-striped'>
    <thead>
      <tr>
        <th>名前</th>
        <th>権限</th>
        <th>登録日時</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td>
            <%= link_to user.display_name, edit_staff_only_user_path(user) %>
          </td>
          <td>
            <%= user.role %>
          </td>
          <td>
            <%= I18n.l(user.created_at) %>
          </td>
        </tr>
      <%end%>
    </tbody>
  </table>
</div>